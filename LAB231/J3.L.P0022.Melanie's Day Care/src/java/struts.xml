<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="" class="Action.getHomeAction">
            <result>Home.jsp</result>
        </action>
        <action name="getHome" class="Action.getHomeAction">
            <result name="success">Home.jsp</result>
        </action>
        <action name="getAbout" class="Action.getAboutAction">
            <result name="success">About me.jsp</result>
        </action>
        <action name="getPhoto" class="Action.getPhotoAction">
            <result name="success">Photoalbum.jsp</result>
        </action>
        <action name="getContact" class="Action.getContactAction">
            <result name="success">Contact.jsp</result>
        </action>
        <action name="getMessage" class="Action.getMessageAction">
            <result name="success">successfulPage.jsp</result>
        </action>
    </package>
</struts>
